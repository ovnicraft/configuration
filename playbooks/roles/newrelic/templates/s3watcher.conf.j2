[program:s3watcher]
command={{ newrelic_s3watcher_dir }}/s3-watcher-supervisor.sh
priority=999
user={{ newrelic_plugin_user }}
stdout_logfile={{ supervisor_log_dir }}/%(program_name)-stdout.log
stderr_logfile={{ supervisor_log_dir }}/%(program_name)-stderr.log
killasgroup=true
stopasgroup=true
stopsignal=QUIT
